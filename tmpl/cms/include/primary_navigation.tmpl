<ul id="primaryNavigation" class="mt-primaryNavigation__list" data-children=".item">
<mt:loop name="top_nav_loop">
  <li class="item mt-primaryNavigation__item">
    <a data-toggle="collapse" data-parent="#primaryNavigation" href="#primayNavigation-item<mt:var name="__counter__">" aria-expanded="false" aria-controls="primayNavigation-item<mt:var name="__counter__">"><mt:var name="label"></a>
    <div id="primayNavigation-item<mt:var name="__counter__">" class="collapse" role="tabpanel">
      <ul class="mt-primaryNavigation__child">
  <mt:loop name="sub_nav_loop">
    <mt:if name="allowed">
        <li><a href="<mt:var name="link" escape="html">"<mt:if name="current"> class="selected"</mt:if>><mt:var name="label"></a></li>
    </mt:if>
  </mt:loop>
      </ul>
    </div>
  </li>
</mt:loop>
</ul>

