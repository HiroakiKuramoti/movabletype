<mt:setvarblock name="page_title"><__trans phrase="An error occurred"></mt:setvarblock>
<mt:setvar name="complete" value="1">
<mt:setvar name="error_page" value="1">

<mt:setvarblock name="page_content">

<mtapp:statusmsg
   id="generic-error"
   class="error"
   can_close="0">
  <p>
    <mt:var name="error" escape="html">
  </p>
<mt:if name="tmpl_loop">
  <ul>
<mt:loop name="tmpl_loop">
    <li><__trans phrase="Content Type">: <mt:var name="name" escape="html">
  <mt:loop name="templates">
      <br />
        <__trans phrase="Template">: <mt:var name="name" escape="html">
        [<a href="?__mode=view&amp;_type=template&amp;id=<mt:var name="id" escape="html">&amp;blog_id=<mt:var name="blog_id" escape="html">">
        <__trans phrase="Edit">
        </a>]
  </mt:loop>
    </li>
</mt:loop>
  </ul>
</mt:if>
</mtapp:statusmsg>

<mt:setvarblock name="action_buttons">
<mt:unless var="hide_goback_button">
<button
   id="goback-button"
   onclick="backSessionHistoryFallback(); <mt:var name="goback">"
   type="submit"
   accesskey="s"
   class="goback action primary button btn btn-primary">
  <mt:var name="value">
</button>
</mt:unless>
</mt:setvarblock>

<mt:include name="include/actions_bar.tmpl" bar_position="bottom" hide_pager="1" settings_bar="1">

</mt:setvarblock>

<mt:include name="layout/chromeless.tmpl">
<mt:var name="layout">
