<mt:setvar name="page_title" value="<__trans phrase="Select Content Type">">
<mt:setvar name="screen_id" value="select-content-type">

<mt:setvarblock name="content">
<mtapp:settinggroup id="content_type">
<ul>
  <mt:loop name="content_types">
    <mt:if name="can_edit">
    <li>
      <a href="<mt:var name="mt_url">?__mode=edit_content_data&blog_id=<mt:var name="blog_id">&content_type_id=<mt:var name="id">">
        <mt:var name="name">
      </a>
    </li>
    </mt:if>
  </mt:loop>
</ul>
</mtapp:settinggroup>

<mt:include name="modules/libjs.tmpl">
</mt:setvarblock>

<mt:include name="layout/scope/blog.tmpl">
<mt:var name="layout">

